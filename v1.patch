diff --git a/components/sections/contact-form.tsx b/components/sections/contact-form.tsx
new file mode 100644
index 0000000000000000000000000000000000000000..5a514bf6dcbafdf2e687027e1d5f98a4b125c399
--- /dev/null
+++ b/components/sections/contact-form.tsx
@@ -0,0 +1,126 @@
+"use client";
+
+import { useState } from "react";
+import { useForm, ValidationError } from "@formspree/react";
+import { Section } from "@/components/sections/section";
+import { Badge } from "@/components/shared/badge";
+import { Input } from "@/components/ui/input";
+import { Textarea } from "@/components/ui/textarea";
+import { Label } from "@/components/ui/label";
+import { Button } from "@/components/ui/button";
+import { Checkbox } from "@/components/ui/checkbox";
+import { site } from "@/content/site";
+
+const formspreeId = process.env.NEXT_PUBLIC_FORMSPREE_ID ?? "";
+const useResend = Boolean(process.env.NEXT_PUBLIC_RESEND_ENABLED);
+const hasFormspree = Boolean(formspreeId);
+
+export function ContactSection() {
+  const [state, handleSubmit] = useForm(formspreeId || "placeholder");
+  const [pilot, setPilot] = useState(false);
+
+  const onSubmit = async (event: React.FormEvent<HTMLFormElement>) => {
+    if (useResend) {
+      event.preventDefault();
+      const form = event.currentTarget;
+      const formData = new FormData(form);
+      formData.set("pilot", pilot ? "Yes" : "No");
+
+      const response = await fetch("/api/send", {
+        method: "POST",
+        body: JSON.stringify(Object.fromEntries(formData)),
+        headers: { "Content-Type": "application/json" }
+      });
+      if (!response.ok) {
+        console.error("Resend submission failed");
+      }
+      form.reset();
+      setPilot(false);
+      return;
+    }
+
+    if (hasFormspree) {
+      await handleSubmit(event);
+      return;
+    }
+
+    event.preventDefault();
+    const form = event.currentTarget as typeof event.currentTarget & {
+      name: { value: string };
+      email: { value: string };
+      business: { value: string };
+      website: { value: string };
+      message: { value: string };
+    };
+
+    const params = new URLSearchParams();
+    params.set("subject", "DialogIX – Intro call request");
+    params.set(
+      "body",
+      `Name: ${form.name.value}\nEmail: ${form.email.value}\nBusiness: ${form.business.value}\nWebsite: ${form.website.value}\nPilot project: ${pilot ? "Yes" : "No"}\n\nMessage:\n${form.message.value}`
+    );
+    window.location.href = `mailto:${site.brand.email}?${params.toString()}`;
+  };
+
+  return (
+    <Section>
+      <div className="grid gap-12 md:grid-cols-[1fr_1fr]">
+        <div className="space-y-6">
+          <Badge>Start the conversation</Badge>
+          <h2 className="text-3xl font-semibold">Let’s scope your pilot in 15 minutes</h2>
+          <p className="text-muted-foreground">
+            Tell us about your busiest channels and what would save your team the most time. We’ll reply within one business
+            day.
+          </p>
+          <div className="rounded-3xl border border-border/50 bg-muted/30 p-6 text-sm text-muted-foreground">
+            <p className="font-medium text-foreground">Calendar placeholder</p>
+            <p className="mt-2">Drop your Calendly, SavvyCal, or Google appointment embed here when ready.</p>
+          </div>
+        </div>
+        <form onSubmit={onSubmit} className="space-y-6">
+          <div className="grid gap-4 md:grid-cols-2">
+            <div className="space-y-2">
+              <Label htmlFor="name">Name</Label>
+              <Input id="name" name="name" required placeholder="Your name" />
+              {hasFormspree && <ValidationError prefix="Name" field="name" errors={state.errors} />}
+            </div>
+            <div className="space-y-2">
+              <Label htmlFor="email">Email</Label>
+              <Input id="email" type="email" name="email" required placeholder="you@business.com" />
+              {hasFormspree && <ValidationError prefix="Email" field="email" errors={state.errors} />}
+            </div>
+          </div>
+          <div className="grid gap-4 md:grid-cols-2">
+            <div className="space-y-2">
+              <Label htmlFor="business">Business</Label>
+              <Input id="business" name="business" placeholder="Salon, restaurant, clinic…" />
+            </div>
+            <div className="space-y-2">
+              <Label htmlFor="website">Website</Label>
+              <Input id="website" name="website" type="url" placeholder="https://" />
+            </div>
+          </div>
+          <div className="space-y-2">
+            <Label htmlFor="message">What are you looking to automate?</Label>
+            <Textarea id="message" name="message" required placeholder="Share workflows, platforms, or pain points." />
+          </div>
+          <div className="flex items-center gap-3">
+            <Checkbox id="pilot" checked={pilot} onCheckedChange={(value) => setPilot(Boolean(value))} />
+            <Label htmlFor="pilot" className="text-sm text-muted-foreground">
+              I’m open to a discounted pilot project
+            </Label>
+          </div>
+          <input type="hidden" name="pilot" value={pilot ? "Yes" : "No"} />
+          <Button type="submit" size="lg" disabled={hasFormspree && state.submitting} className="w-full md:w-auto">
+            {hasFormspree && state.submitting ? "Sending..." : "Send message"}
+          </Button>
+          {!useResend && !hasFormspree && (
+            <p className="text-xs text-muted-foreground">
+              No Formspree ID detected. The button will open your email client.
+            </p>
+          )}
+        </form>
+      </div>
+    </Section>
+  );
+}
