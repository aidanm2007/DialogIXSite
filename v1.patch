+          </div>
+          <div className="flex gap-3">
+            <Button variant="outline" onClick={prev} aria-label="Previous testimonial">
+              Prev
+            </Button>
+            <Button variant="outline" onClick={next} aria-label="Next testimonial">
+              Next
+            </Button>
+          </div>
+        </div>
