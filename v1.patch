@@ -0,0 +1,26 @@
+import { NextResponse } from "next/server";
+import { Resend } from "resend";
+import { site } from "@/content/site";
+
+export async function POST(request: Request) {
+  if (!process.env.RESEND_API_KEY) {
+    return NextResponse.json({ error: "Resend not configured" }, { status: 400 });
+  }
+
+  const resend = new Resend(process.env.RESEND_API_KEY);
+  const body = await request.json();
+
+  try {
+    await resend.emails.send({
+      from: `DialogIX <no-reply@dialogix.agency>`,
+      to: [site.brand.email],
+      subject: "New inquiry from DialogIX website",
+      replyTo: body.email,
+      text: `Name: ${body.name}\nEmail: ${body.email}\nBusiness: ${body.business}\nWebsite: ${body.website}\nPilot: ${body.pilot}\n\nMessage:\n${body.message}`
+    });
+    return NextResponse.json({ status: "ok" });
+  } catch (error) {
+    console.error(error);
+    return NextResponse.json({ error: "Failed to send" }, { status: 500 });
+  }
+}
